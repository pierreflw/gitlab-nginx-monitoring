networks:
  "{{ docker_network }}":
    external: true
  
  
services:
    gitlab-runner:
      image: gitlab/gitlab-runner:latest
      container_name: gitlab-runner
      networks:
      - "{{ docker_network }}"
      restart: always
      volumes:
        - /srv/gitlab-runner/config:/etc/gitlab-runner
        - /var/run/docker.sock:/var/run/docker.sock  