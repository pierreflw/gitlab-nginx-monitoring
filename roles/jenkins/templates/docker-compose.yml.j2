networks:
  "{{ docker_network }}":
    external: true

services:
  jenkins:
    image: "jenkins/jenkins"
    container_name: jenkins
    networks: 
      - "{{ docker_network }}"
    restart: unless-stopped
    privileged: true
    user: root
    ports:
        - "8081:8080"
        - "50000:50000"
    volumes:
        - "{{ jenkins_data_path }}:/var/jenkins_home"